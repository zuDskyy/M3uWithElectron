{
  "name": "react-m3u8-electron",
  "version": "0.1.0",
  "homepage": "./",
  "description": "M3u Electron",
  "main": "./public/electron.js",
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.11.12",
    "@mui/styled-engine-sc": "^5.11.11",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.3.4",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^5.3.0",
    "m3u-parser-generator": "^1.2.0",
    "m3u8": "^0.0.10",
    "m3u8-parser": "^6.0.0",
    "m3u8-react-player": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-player": "^2.12.0",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "semantic-ui-css": "^2.5.0",
    "styled-components": "^5.3.8",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:start": "concurrently -k \"cross-env BROWSER=none yarn start\" \"electron .\"",
    "electron:package:mac": "yarn build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:win": "yarn build && electron-builder --win -c.extraMetadata.main=build/electron.js --publish never",
    "electron:package:linux": "yarn build && electron-builder -l -c.extraMetadata.main=build/electron.js ",
    "deploy": " yarn build && electron-builder   --win --publish always"
  },
  "build": {
    "generateUpdatesFilesForAllChannels": true,
    "appId": "com.electron.myapp",
    "productName": "Z-Player",
    "copyright": "zuDskyy",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": "dmg",
      "publish": {
        "provider": "github",
        "owner": "zuDskyy",
        "repo": "m3u-player-electron",
        "private": true,
        "token": "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"
      }
    },
    "win": {
      "target": "nsis",
      "publish": {
        "provider": "github",
        "owner": "zuDskyy",
        "repo": "m3u-player-electron",
        "private": true,
        "token": "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"
      },
      "icon": "./build/player.ico"
    },
    "linux": {
      "target": "deb",
      "publish": {
        "provider": "github",
        "owner": "zuDskyy",
        "private": true,
        "repo": "m3u-player-electron",
        "token": "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"
      }
    },
    "publish": {
      "provider": "github",
      "owner": "zuDskyy"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^23.1.4",
    "electron-builder": "^23.6.0",
    "electronmon": "^2.0.2"
  }
}
